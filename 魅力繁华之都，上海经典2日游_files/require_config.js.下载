                self._ShowTip('å¯åœ¨APPé¦–é¡µåº•éƒ¨è¡Œç¨‹æŸ¥çœ‹');
                //$(".js_add_travel_plan_suc").children().find('.cui-layer-content').find('h2').html('å·²æ·»åŠ åˆ°è¡Œç¨‹');
                //$(".js_add_travel_plan_suc").children().find('.cui-layer-content').find('h4').html('å¯åœ¨APPé¦–é¡µåº•éƒ¨è¡Œç¨‹æŸ¥çœ‹');
            } else {
                $(".js_add_travel_plan_background").addClass("opacity-6").show();
                self.forbidBodyScroll();
                $(".js_add_travel_plan_suc").data('travelplanid', travelPlanId);
                

                    //$('.js_add_travel_plan_suc').click(function () {
