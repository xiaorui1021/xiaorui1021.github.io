                        $('.js_image_fly').hide();
                    }, 2000);
                })

            //});
            $('.cui-white-layer-padding').undelegate().delegate(".js_add_travel_plan_suc_close", 'click', function (e) {
                $('.js_add_travel_plan_background').hide();
                self.allowBodyScroll();
                $('.js_add_travel_plan_suc').hide();
                $('.js_image_fly').show();
                window.setTimeout(function () {
                    $('.js_image_fly').hide();
                }, 2000);
                e.stopImmediatePropagation();
                return false;
            });
            $(".js_add_travel_plan_suc").children().find('.cui-layer-content').find('h2').html('å·²æ·»åŠ åˆ°æ—…è¡Œè®¡åˆ’');
            $(".js_add_travel_plan_suc").children().find('.cui-layer-content').find('h4').html('æ ‡é¢˜ä¸º"' + self.params.bussinessName + '"å»æŸ¥çœ‹');
            $(".js_add_travel_plan_suc").show();

            gscommon.cTrackLog(100664, {
                actioncode: 'poi_plus_plan_success'
            });

        },

        gotoTravelPage: function (travelPlanId) {

                routerRedirect.toJump({
                    url: [Lizard.H5BaseUrl + "/webapp/you/journeys/travelplan/list.html?from=" + window.location.href, "destination/journey/index.html#travelplan/list.html"],
                    targetModel: 4
                });

        },

        _nodate: function () {
            var self = this;

            $('.js_schedule').undelegate().delegate(".js_schedule_no_date", 'click', function (e) {
                addTravelPlanModel.setParam({
                    Guid: new self.getGUID().newGUID(),
                    PoiId: self.params.poiId,
                    TravelPlanId: 0,
                    playDate: null
                });
                addTravelPlanModel.excute(function (data) {
                    if (data && data.ResponseStatus && data.ResponseStatus.Ack == 'Success') {

                        self._ShowTip73('æ ‡é¢˜ä¸º' + self.params.bussinessName);
                    } else {
                        Lizard.showToast("åŠ å…¥æ—…è¡Œè®¡åˆ’å¤±è´¥");
                    }
                    self._hideCalendar();
                });




                self._hideCalendar();
            });
        },

        forbidBodyScroll: function () {
            $(".js_add_travel_plan_background").on('touchmove', function (e) {
                e.stopImmediatePropagation();
                e.preventDefault;
                return false;
            });
        },

        allowBodyScroll: function () {
            $(".js_add_travel_plan_background").off('touchmove');
        },

        getGUID: function GUID() {
            this.date = new Date();

            /* åˆ¤æ–­æ˜¯å¦åˆå§‹åŒ–è¿‡ï¼Œå¦‚æœåˆå§‹åŒ–è¿‡ä»¥ä¸‹ä»£ç ï¼Œåˆ™ä»¥ä¸‹ä»£ç å°†ä¸å†æ‰§è¡Œï¼Œå®é™…ä¸­åªæ‰§è¡Œä¸€æ¬¡ */
            if (typeof this.newGUID != 'function') {

                /* ç”ŸæˆGUIDç  */
                GUID.prototype.newGUID = function () {
                    this.date = new Date();
                    var guidStr = '';
                    sexadecimalDate = this.hexadecimal(this.getGUIDDate(), 16);
                    sexadecimalTime = this.hexadecimal(this.getGUIDTime(), 16);
                    for (var i = 0; i < 9; i++) {
                        guidStr += Math.floor(Math.random() * 16).toString(16);
                    }
                    guidStr += sexadecimalDate;
                    guidStr += sexadecimalTime;
                    while (guidStr.length < 32) {
                        guidStr += Math.floor(Math.random() * 16).toString(16);
                    }
                    return this.formatGUID(guidStr);
                }

                /*
     Â Â Â Â Â * åŠŸèƒ½ï¼šè·å–å½“å‰æ—¥æœŸçš„GUIDæ ¼å¼ï¼Œå³8ä½æ•°çš„æ—¥æœŸï¼š19700101
     Â Â Â Â Â * è¿”å›å€¼ï¼šè¿”å›GUIDæ—¥æœŸæ ¼å¼çš„å­—æ¡ä¸²
     Â Â Â Â Â */
                GUID.prototype.getGUIDDate = function () {
                    return this.date.getFullYear() + this.addZero(this.date.getMonth() + 1) + this.addZero(this.date.getDay());
                }

                /*
     Â Â Â Â Â * åŠŸèƒ½ï¼šè·å–å½“å‰æ—¶é—´çš„GUIDæ ¼å¼ï¼Œå³8ä½æ•°çš„æ—¶é—´ï¼ŒåŒ…æ‹¬æ¯«ç§’ï¼Œæ¯«ç§’ä¸º2ä½æ•°ï¼š12300933
     Â Â Â Â Â * è¿”å›å€¼ï¼šè¿”å›GUIDæ—¥æœŸæ ¼å¼çš„å­—æ¡ä¸²
     Â Â Â Â Â */
                GUID.prototype.getGUIDTime = function () {
                    return this.addZero(this.date.getHours()) + this.addZero(this.date.getMinutes()) + this.addZero(this.date.getSeconds()) + this.addZero(parseInt(this.date.getMilliseconds() / 10));
                }

                /*
     Â Â Â Â   * åŠŸèƒ½: ä¸ºä¸€ä½æ•°çš„æ­£æ•´æ•°å‰é¢æ·»åŠ 0ï¼Œå¦‚æœæ˜¯å¯ä»¥è½¬æˆéNaNæ•°å­—çš„å­—ç¬¦ä¸²ä¹Ÿå¯ä»¥å®ç°
     Â Â Â Â Â * å‚æ•°: å‚æ•°è¡¨ç¤ºå‡†å¤‡å†å‰é¢æ·»åŠ 0çš„æ•°å­—æˆ–å¯ä»¥è½¬æ¢æˆæ•°å­—çš„å­—ç¬¦ä¸²
     Â Â Â Â Â * è¿”å›å€¼: å¦‚æœç¬¦åˆæ¡ä»¶ï¼Œè¿”å›æ·»åŠ 0åçš„å­—æ¡ä¸²ç±»å‹ï¼Œå¦åˆ™è¿”å›è‡ªèº«çš„å­—ç¬¦ä¸²
     Â Â Â Â Â */
                GUID.prototype.addZero = function (num) {
                    if (Number(num).toString() != 'NaN' && num >= 0 && num < 10) {
                        return '0' + Math.floor(num);
                    } else {
                        return num.toString();
                    }
                }

                /*
     Â Â Â Â Â * åŠŸèƒ½ï¼šå°†yè¿›åˆ¶çš„æ•°å€¼ï¼Œè½¬æ¢ä¸ºxè¿›åˆ¶çš„æ•°å€¼
     Â Â Â Â Â * å‚æ•°ï¼šç¬¬1ä¸ªå‚æ•°è¡¨ç¤ºæ¬²è½¬æ¢çš„æ•°å€¼ï¼›ç¬¬2ä¸ªå‚æ•°è¡¨ç¤ºæ¬²è½¬æ¢çš„è¿›åˆ¶ï¼›ç¬¬3ä¸ªå‚æ•°å¯é€‰ï¼Œè¡¨ç¤ºå½“å‰çš„è¿›åˆ¶æ•°ï¼Œå¦‚ä¸å†™åˆ™ä¸º10
     Â Â Â Â Â * è¿”å›å€¼ï¼šè¿”å›è½¬æ¢åçš„å­—ç¬¦ä¸²
     Â Â Â Â Â */
                GUID.prototype.hexadecimal = function (num, x, y) {
                    if (y != undefined) {
                        return parseInt(num.toString(), y).toString(x);
                    } else {
                        return parseInt(num.toString()).toString(x);
                    }
                }

                /*
     Â Â Â Â Â * åŠŸèƒ½ï¼šæ ¼å¼åŒ–32ä½çš„å­—ç¬¦ä¸²ä¸ºGUIDæ¨¡å¼çš„å­—ç¬¦ä¸²
     Â Â Â Â Â * å‚æ•°ï¼šç¬¬1ä¸ªå‚æ•°è¡¨ç¤º32ä½çš„å­—ç¬¦ä¸²
     Â Â Â Â Â * è¿”å›å€¼ï¼šæ ‡å‡†GUIDæ ¼å¼çš„å­—ç¬¦ä¸²
     Â Â Â Â Â */
                GUID.prototype.formatGUID = function (guidStr) {
                    var str1 = guidStr.slice(0, 8) + '-',
                        str2 = guidStr.slice(8, 12) + '-',
                        str3 = guidStr.slice(12, 16) + '-',
                        str4 = guidStr.slice(16, 20) + '-',
                        str5 = guidStr.slice(20);
                    return str1 + str2 + str3 + str4 + str5;
                }
            }
        }
    };

    return addCalendar;
});

